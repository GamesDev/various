<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs>
    <Require feature="flash" />
  </ModulePrefs>
  <Content type="html" view="canvas">
    <![CDATA[
      <script type="text/javascript">
        function displayFlash() {
          var params = {
            width: 760,
            height: 500,
            allowScriptAccess: 'always'
          };
          var swfUrl = 'http://www.biology.ualberta.ca/facilities/multimedia/uploads/alberta/Ecosystem.swf';
          gadgets.flash.embedFlash(swfUrl, 'flash-container', '10', params);
        }

        gadgets.util.registerOnLoadHandler(displayFlash);
      </script>
      <div id="flash-container"></div>
    ]]>
  </Content>
</Module>

